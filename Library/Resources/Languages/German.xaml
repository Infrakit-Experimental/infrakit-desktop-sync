<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!--#region general-->

    <sys:String x:Key="ok">OK</sys:String>
    <sys:String x:Key="cancel">Abbruch</sys:String>

    <!--#endregion general-->

    <!--#region windows-->

    <!--#region log in window-->

    <sys:String x:Key="logIn.username">Benutzername:</sys:String>
    <sys:String x:Key="logIn.password">Passwort:</sys:String>
    <sys:String x:Key="logIn.rememberMe">Benutzername merken</sys:String>
    <sys:String x:Key="logIn.logIn">Einloggen</sys:String>

    <!--#endregion log in window-->

    <!--#region settings window-->

    <sys:String x:Key="settings.title">Einstellungen</sys:String>
    <sys:String x:Key="settings.language">Sprache:</sys:String>

    <!--#endregion settings window-->

    <!--#endregion windows-->

    <!--#region API-->

    <!--#region API Key-->

    <sys:String x:Key="api.APIExpired.caption">API Key</sys:String>
    <sys:String x:Key="api.APIExpired.message">API Key Abgelaufen</sys:String>

    <!--#endregion API Key-->

    <!--#region titles-->

    <!--#region authentication-->

    <sys:String x:Key="api.authentication.postApiLogIn">Fehler beim Abrufen des ApiKey</sys:String>

    <!--#endregion authentication-->

    <!--#region project-->

    <sys:String x:Key="api.project.post">Fehler beim Erstellen des Projekts</sys:String>
    <sys:String x:Key="api.project.get">Fehler beim Abrufen der Projekte</sys:String>
    <sys:String x:Key="api.project.getMetadata">Fehler beim Abrufen der Projektmetadaten</sys:String>
    <sys:String x:Key="api.project.getFolders">Fehler beim Abrufen der Ordner</sys:String>

    <!--#endregion project-->

    <!--#region folder-->

    <sys:String x:Key="api.folder.get">Fehler beim Abrufen der Ordner und/oder Dokumente</sys:String>
    <sys:String x:Key="api.folder.getMetadata">Fehler beim Abrufen der Metadaten des Ordners</sys:String>
    <sys:String x:Key="api.folder.post">Fehler beim Erstellen des Ordners</sys:String>
    <sys:String x:Key="api.folder.getDocuments">Fehler beim Abrufen der Dokumente</sys:String>
    <sys:String x:Key="api.folder.getFolders">Fehler beim Abrufen der Ordner</sys:String>
    <sys:String x:Key="api.folder.getImages">Fehler beim Abrufen der Bilder</sys:String>
    <sys:String x:Key="api.folder.getProperties">Fehler beim Abrufen der Eigenschaften des Ordners</sys:String>


    <!--#endregion folder-->

    <!--#region equipment-->

    <sys:String x:Key="api.equipment.get">Fehler beim Abrufen der Maschine</sys:String>

    <!--#endregion equipment-->

    <!--#region document-->

    <sys:String x:Key="api.document.getUploudURL">Fehler beim Abrufen der Datei-Upload-URL</sys:String>
    <sys:String x:Key="api.document.upload">Fehler beim Hochladen der Datei</sys:String>
    <sys:String x:Key="api.document.getMetadata">Fehler beim Abrufen der Dokumentmetadaten</sys:String>
    <sys:String x:Key="api.document.delete">Fehler beim Löschen des Dokuments</sys:String>
    <sys:String x:Key="api.document.getDownloadURL">Fehler beim Abrufen der Dokument-Download-URL</sys:String>
    <sys:String x:Key="api.document.download">Fehler beim Herunterladen der Datei</sys:String>
    
    <!--#endregion document-->

    <!--#region logpoint-->

    <sys:String x:Key="api.logpoint.get">Fehler beim Abrufen der Messpunkte</sys:String>

    <!--#endregion logpoint-->

    <!--#region property-->

    <sys:String x:Key="api.property.getProject">Fehler beim Abrufen der Eigenschaften des Projekts</sys:String>
    <sys:String x:Key="api.property.addProject">Fehler beim Hinzufügen der Eigenschaften zum Projekt</sys:String>

    <!--#endregion property-->

    <!--#region undocumented endpoints-->

    <sys:String x:Key="api.deleteFileFolder">Fehler beim Löschen des Ordners</sys:String>

    <!--#endregion undocumented endpoints-->

    <!--#endregion titles-->

    <!--#region messages-->

    <sys:String x:Key="api.default">Ein unerwarteter Fehler ist aufgetreten</sys:String>

    <!--#region status codes-->

    <sys:String x:Key="api.400.1">Die angegebenen Eingabeparameter fehlen oder sind ungültig</sys:String>
    <sys:String x:Key="api.400.2">Die Anfrage war ungültig oder enthielt fehlerhafte Parameter</sys:String>

    <sys:String x:Key="api.401">Konnte nicht authentifizieren</sys:String>

    <sys:String x:Key="api.403.1">Dem Sitzungsbenutzer wurde der Zugriff auf das Zielprojekt verweigert</sys:String>
    <sys:String x:Key="api.403.2">Authentifizierung fehlgeschlagen (ungültige Anmeldeinformationen)</sys:String>
    <sys:String x:Key="api.403.3">Der Benutzer wurde authentifiziert, ihm fehlen jedoch Berechtigungen</sys:String>
    <sys:String x:Key="api.403.4">Keine Berechtigung zum Erstellen von Objekten</sys:String>

    <sys:String x:Key="api.404.1">Projekt existiert nicht</sys:String>
    <sys:String x:Key="api.404.2">Ordner existiert nicht</sys:String>
    <sys:String x:Key="api.404.3">Dokument existiert nicht</sys:String>
    <sys:String x:Key="api.404.4">Aufgabe nicht in der Datenbank gefunden</sys:String>

    <sys:String x:Key="api.409">Das identische Dokument existiert bereits</sys:String>

    <sys:String x:Key="api.500">Die interne Infrakit-Ressource ist fehlgeschlagen</sys:String>

    <!--#endregion status codes-->

    <sys:String x:Key="api.timeout">Es ist ein Timeout-Fehler aufgetreten</sys:String>

    <!--#endregion messages-->

    <!--#endregion API-->

</ResourceDictionary>